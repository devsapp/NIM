while true; do currentParakeet="$(s cli fc provision get --qualifier LATEST --target current 1 --service-name ASR --function-name parakeet --region cn-shanghai -a {{ access }} | tr -d ' ' | awk -F ':' '/current:/ {print $2}' | sed 's/\x1b\[[0-9;]*m//g')"; if [ "$currentParakeet" == '1' ]; then break; else echo "wait provision instance ready, currentParakeet $currentParakeet;" && sleep 5; fi; done;